#This os excercise bout numbers and math

import sys

print "I will now count my chickens:"
print "Hens:", 25 + 30 / 6
print "Roosters:", 100 - 25 * (3 % 4)
print "Now I will count the eggs:"
print 3 + 2 + 1 - 5 + 4 % 2 - 1 / 4 + 6
sys.stdout.write ("Is it true than 3 + 2 < 5 - 7?")
print 3 + 2 < 5 - 7
sys.stdout.write(" It should be in the same line as is it true ...") 
print "What is 3 + 2?", 3 + 2
print "What is 5 - 7?", 5 - 7
print "Oh, that's why it is False."
print "How about some more."
print "is it greater?", 5 > -2
print "Is it greater or equal?", 5 >= -2
print "It is less or equal?", 5 <= -2
print "Is it modulo?", 5%3==2 
print "<=true | false=>",   5%3==1
print "Divsision 5/3", 5 / 3
print "Division 5.0/3", 5.0/3
